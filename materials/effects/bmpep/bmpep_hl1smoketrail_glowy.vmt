"unlitgeneric"
{
	"$basetexture" "sprites/smoke"
	"$vertexcolor" 1
	"$vertexalpha" 1
	 $additive 1
	$overbrightfactor 3
        $HDRCOLORSCALE 3
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 15
		}

	}
}
